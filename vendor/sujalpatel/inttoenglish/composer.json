{
  "name": "sujalpatel/inttoenglish",
  "description": "Convert Integers to English Words",
  "keywords": [
    "sujalpatel",
    "inttoenglish",
    "int2english",
    "converter"
  ],
  "homepage": "https://github.com/sujalpatel2209/inttoenglish",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Sujal Patel",
      "email": "sujalpatel022@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "orchestra/testbench": "4.3.*",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "SujalPatel\\IntToEnglish\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SujalPatel\\IntToEnglish\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "SujalPatel\\IntToEnglish\\IntToEnglishServiceProvider"
      ],
      "aliases": {
        "IntToEnglish": "SujalPatel\\IntToEnglish\\IntToEnglishFacade"
      }
    }
  }
}
